n    if (screen === screens.feed)\n      return <Feed />\n  }\n\n  render() {\n    return (\n      <div>\n        <Navbar setScreen={(screen) => this.setState({ screen })} />\n        {this.selectScreen()}\n      </div>\n    );\n  }\n}\n\nexport default withAuthenticator(App, { includeGreetings: true });\n","/Users/haroun.ansari/react-amplify-app/src/aws-exports.js",[],"/Users/haroun.ansari/react-amplify-app/src/Components/AddPhoto.jsx",[],"/Users/haroun.ansari/react-amplify-app/src/Components/GetPhoto.jsx",["106"],"import React, { Component } from 'react';\nimport { Auth, Storage } from 'aws-amplify';\n\n\nclass GetPhoto extends Component {\n    constructor(props) {\n        super(props);\n        \n        this.state = {\n            fileUrl: ''\n        };\n    }\n    componentDidMount = async () => {\n        try {\n            const data = await Storage.get('photos/husie/2021-01-24T22:46:32.759Z.jpeg');\n            this.setState({ fileUrl: data });\n        } catch (error) {\n            console.log('error fetching image', error);\n        }\n    }\n    render() {\n        return (\n            <>\n                <img src={this.state.fileUrl} alt=\"\"/>\n            </>\n        );\n    }\n}\n\nexport default GetPhoto;",["107","108"],"/Users/haroun.ansari/react-amplify-app/src/AppContext.js",[],"/Users/haroun.ansari/react-amplify-app/src/Components/Loading.jsx",[],"/Users/haroun.ansari/react-amplify-app/src/Components/MyProfile.jsx",[],"/Users/haroun.ansari/react-amplify-app/src/config.js",[],"/Users/haroun.ansari/react-amplify-app/src/Components/Grid.jsx",[],"/Users/haroun.ansari/react-amplify-app/src/Components/UserProfileInfo.jsx",["109","110"],"import React, { Component } from 'react';\n\nclass UserProfileInfo extends Component {\n    render() {\n        const {\n            username,\n            email,\n            followers,\n            following,\n            phone,\n            timestamp\n        } = this.props.user;\n\n        return (\n           