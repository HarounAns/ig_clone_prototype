from 'react';\nimport axios from 'axios';\nimport { Auth } from 'aws-amplify';\n\nimport config from '../config';\nimport Loading from './Loading';\nimport UserProfile from './UserProfile';\n\nconst { PRODUCTDB_API_URL } = config;\n\nclass FollowerList extends Component {\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            loading: true\n        };\n    }\n\n    componentDidMount = async () => {\n        await this.getFollowers();\n    }\n\n    getFollowers = async () => {\n        const { username } = await Auth.currentAuthenticatedUser();\n        const res = await axios.get(PRODUCTDB_API_URL + `/users/${username}/followers`);\n        const { Followers } = res.data;\n\n        this.setState({\n            Followers,\n            loading: false\n        });\n    }\n\n    searchUser = (username) => {\n        this.setState({\n            showProfile: true,\n            username\n        })\n    }\n\n    render() {\n        const { Followers, loading, showProfile, username } = this.state;\n\n        if (loading)\n            return <Loading />\n\n        if (showProfile) {\n            return <UserProfile username={username} />\n        }\n\n        return (\n            <div>\n                <h5>Followers:</h5>\n                {Followers.map(follower =>\n                    <p>\n                        <a href=\"#\" onClick={() => this.searchUser(follower.followingUser)}>\n                            {follower.followingUser}\n                        </a>\n                    </p>\n                )}\n                <button onClick={this.props.goBack}>Back</button>\n            </div>\n        )\n    }\n}\n\nexport default FollowerList;","/Users/haroun.ansari/react-amplify-app/src/Components/FollowingList.jsx",["112"],"import React, { Component } from 'react';\nimport axios from 'axios';\nimport { Auth } from 'aws-amplify';\n\nimport config from '../config';\nimport Loading from './Loading';\nimport UserProfile from './UserProfile';\n\nconst { PRODUCTDB_API_URL } = config;\n\nclass FollowingList extends Component {\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            loading: true\n        };\n    }\n\n    componentDidMount = async () => {\n        await this.getFollowing();\n    }\n\n    getFollowing = async () => {\n        const { username } = await Auth.currentAuthenticatedUser();\n        const res = await axios.get(PRODUCTDB_API_URL + `/users/${username}/following`);\n        const { Following } = res.data;\n\n        this.setState({\n            Following,\n            loading: false\n        });\n    }\n\n    searchUser = (username) => {\n        this.setState({\n            showProfile: true,\n            username\n        })\n    }\n\n    render() {\n        const { Following, loading, showProfile, username } = this.state;\n\n        if (loading)\n            return <Loading />\n\n        if (showProfile) {\n            return <UserProfile username={username} />\n        }\n\n        return (\n            <div>\n                <h5>Following:</h5>\n                {Following.map(follower =>\n                    <p>\n                        <a href=\"#\" onClick={() => this.searchUser(follower.followedUser)}>\n                            {follower.followedUser}\n                        </a>\n                    </p>\n                )}\n                <button onClick={this.props.goBack}>Back</button>\n            </div>\n        )\n    }\n}\n\nexport default FollowingList;",["113","114"],"/Users/haroun.ansari/react-amplify-app/src/Components/NotificationBar.jsx",["115"],"import { divide } from 'lodash';\nimport React, { Component } from 'react';\nimport { AppContext } from '../AppContext';\n\nclass NotificationBar extends Component {\n    static contextType = AppContext;\n\n    componentWillUnmount  = () => {\n        this.context.setNotification([]);\n    }\n\n    render() {\n        const { notifications } = this.context;\n        return notifications.map(notification => {\n            const {\n                type,\n                post,\n                likingUser,\n                followingUser,\n                timestamp\n            } = notification;\n\n            if (type === 'like') {\n                return (\n                    <div>\n                        <p>{`${likingUser} liked your post ${post} at ${timestamp}`}</p>\n                    </div>\n                )\n            }\n\n            if (type === 'follow') {\n                return (\n                    <div>\n                        <p>{`${followingUser} just followed you! ${timestamp}`}</p>\n                    </div>\n                )\n            }\n\n            return null;\n        });\n    }\n}\n\nexport default NotificationBar;",{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","severity":1,"message":"121","line":3,"column":10,"nodeType":"122","messageId":"123","endLine":3,"endColumn":20},{"ruleId":"120","severity":1,"message":"124","line":2,"column":10,"nodeType":"122","messageId":"123","endLine":2,"endColumn":14},{"ruleId":"116","replacedBy":"125"},{"ruleId":"118","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":27,"column":29,"nodeType":"129","endLine":27,"endColumn":78},{"ruleId":"127","severity":1,"message":"128","line":35,"column":29,"nodeType":"129","endLine":35,"endColumn":78},{"ruleId":"127","severity":1,"message":"128","line":57,"column":25,"nodeType":"129","endLine":57,"endColumn":93},{"ruleId":"127","severity":1,"message":"128","line":57,"column":25,"nodeType":"129","endLine":57,"endColumn":92},{"ruleId":"116","replacedBy":"130"},{"ruleId":"118","replacedBy":"131"},{"ruleId":"120","severity":1,"message":"132","line":1,"column":10,"nodeType":"122","messageId":"123","endLine":1,"endColumn":16},"no-native-reassign",["133"],"no-negated-in-lhs",["134"],"no-unused-vars","'AppContext' is defined but never used.","Identifier","unusedVar","'Auth' is defin